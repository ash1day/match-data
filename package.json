{
  "name": "@tn/match-data",
  "version": "1.0.0",
  "description": "TFT match data collection from Riot API",
  "packageManager": "yarn@4.11.0",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch src/index-s3.ts",
    "build": "tsc",
    "collect-players": "tsx src/index-players.ts",
    "collect-matches": "tsx src/index-s3.ts",
    "s3:download": "tsx src/sync-s3.ts download",
    "s3:upload": "tsx src/sync-s3.ts upload",
    "s3:status": "tsx src/sync-s3.ts status",
    "lint": "biome check src/",
    "fix": "biome check --write src/",
    "fb": "biome check --write src/"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.864.0",
    "dotenv": "^16.0.0",
    "duckdb-async": "^1.3.1",
    "tsx": "^4.7.1",
    "twisted": "^1.63.0",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.5",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  }
}
